{
    "runtime": "Net80",
    "documentGenerator": {
        "fromDocument": {
            "url": "http://localhost:5188/swagger/v1/swagger.json"
        }
    },
    "codeGenerators": {
        "openApiToTypeScriptClient": {
            "template": "Fetch",
            "typeStyle": "Class",
            "useAbortSignal": true,
            "operationGenerationMode": "SingleClientFromOperationId",
            "clientClassName": "Client",
            "output": "../cliq.client/services/generated/generatedClient.ts"
        }
    }
}